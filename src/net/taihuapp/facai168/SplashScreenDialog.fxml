<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.*?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.taihuapp.facai168.SplashScreenDialogController">
   <center>
      <TextArea fx:id="mTextArea" editable="false" prefHeight="200.0" prefWidth="200.0" wrapText="true" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <TilePane alignment="CENTER" prefColumns="3" prefRows="1" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <CheckBox fx:id="mAgreeCheckBox" mnemonicParsing="false" text="I Agree.">
               <TilePane.margin>
                  <Insets bottom="5.0" left="5.0" right="50.0" top="5.0" />
               </TilePane.margin>
            </CheckBox>
            <Button fx:id="mContinueButton" mnemonicParsing="false" onAction="#handleContinue" text="Continue">
               <TilePane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </TilePane.margin>
            </Button>
            <Button fx:id="mStopButton" mnemonicParsing="false" onAction="#handleStop" text="Stop">
               <TilePane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </TilePane.margin>
            </Button>
         </children>
      </TilePane>
   </bottom>
</BorderPane>
